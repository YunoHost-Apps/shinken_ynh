{
    "name": "Shinken",
    "id": "shinken",
    "packaging_format": 1,
    "description": {
        "en": "Flexible and scalable monitoring framework",
        "fr": "Framework de monitoring flexible et scalable"
    },
    "version": "2.4.3~ynh1",
    "url": "https://github.com/naparuba/shinken",
    "license": "AGPL-3.0",
    "maintainer": {
        "name": "liberodark",
        "email": "liberodark@gmail.com"
    },
    "requirements": {
        "yunohost": ">= 4.2.4"
    },
    "multi_instance": false,
    "services": [
        "nginx",
        "php7.3-fpm",
        "mysql"
    ],
    "arguments": {
        "install": [{
                "name": "domain",
                "type": "domain",
                "example": "example.com"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
